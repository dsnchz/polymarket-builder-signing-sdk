{
  "name": "@dschz/polymarket-builder-signing-sdk",
  "description": "Polymarket SDK for creating builder headers",
  "version": "0.3.0",
  "type": "module",
  "author": "Daniel Sanchez <dsanc89@pm.me>",
  "license": "MIT",
  "contributors": [
    {
      "name": "Daniel Sanchez",
      "url": "https://github.com/dsnchz"
    },
    {
      "name": "Jonathan Amenechi",
      "url": "https://github.com/JonathanAmenechi"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "pkg:changeset": "changeset",
    "pkg:version": "changeset version",
    "pkg:publish": "bun run build && changeset publish",
    "test": "bun test",
    "test:cov": "bun test --coverage",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.7",
    "@eslint/js": "9.38.0",
    "@tsconfig/strictest": "2.0.7",
    "@types/bun": "^1.3.1",
    "@types/node": "22.18.4",
    "@types/sinon": "^17.0.4",
    "dotenv": "^16.0.2",
    "eslint": "9.35.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "16.4.0",
    "jiti": "2.6.1",
    "path": "^0.12.7",
    "prettier": "^3.6.2",
    "sinon": "^21.0.0",
    "tsup": "8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "8.46.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "bun@>=1.3.0"
}
